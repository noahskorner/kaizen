{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "lint:fix": {},
    "test": {
      "cache": false
    },
    "test:e2e": {},
    "dev": {
      "cache": false,
      "persistent": false
    }
  }
}
