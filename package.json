{
  "private": true,
  "name": "kaizen",
  "packageManager": "npm@10.2.2",
  "workspaces": [
    "apps/api",
    "apps/frontend",
    "packages/**"
  ],
  "volta": {
    "node": "20.9.0",
    "npm": "10.2.2"
  },
  "prisma": {
    "schema": "packages/data/src/prisma/schema.prisma"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier . --write",
    "clean": "find . -type d ( -name 'node_modules' -o -name '.turbo' -o -name 'dist' -o -name 'cdk.out' -o -name '.next' ) -exec rm -rf {} +"
